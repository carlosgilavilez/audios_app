APP_NAME="Audios IBRPM"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://audios.iglesiapalma.org

LOG_CHANNEL=stack
LOG_LEVEL=info

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=audios_db
DB_USERNAME=audios_user
DB_PASSWORD=secret

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=public
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

VITE_APP_NAME="${APP_NAME}"
VITE_APP_URL="${APP_URL}"

# CORS for WP and subdomain
CORS_ALLOWED_ORIGINS=https://iglesiapalma.org,https://audios.iglesiapalma.org
# Allowed frame ancestors for the public embed endpoint (space or comma separated)
EMBED_FRAME_ANCESTORS="https://iglesiapalma.org https://www.iglesiapalma.org"
